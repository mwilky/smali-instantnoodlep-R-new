<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:dither="true" android:shape="oval">
            <stroke android:width="@dimen/seekbar_thumb_circle_width" android:color="@color/light_blue" />
            <size android:height="@dimen/seekbar_thumb_outer_diameter" android:width="@dimen/seekbar_thumb_outer_diameter" />
        </shape>
    </item>
    <item android:left="@dimen/seekbar_thumb_circle_width" android:top="@dimen/seekbar_thumb_circle_width" android:right="@dimen/seekbar_thumb_circle_width" android:bottom="@dimen/seekbar_thumb_circle_width">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
            <size android:height="@dimen/seekbar_thumb_inner_diameter" android:width="@dimen/seekbar_thumb_inner_diameter" />
        </shape>
    </item>
</layer-list>